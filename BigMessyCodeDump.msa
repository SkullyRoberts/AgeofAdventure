/buttonmsg $value [$msg='null'] = >>>
        @array=get_value(buttons,announcements.broadcast)
        @index=@array[index]
        if(array_contains(@index,$value)){
                msg(colorize('Current message is:' @array[$value]))
                if($msg=='clear'){
                        @newarray1=array_remove_values(@array,$value)
                        @newindex=array_remove_values(@index,$value)
                        @newarray2=array_set(@array,index,@newindex)
                        store_value(buttons,announcements.broadcast,@newarray2)
                }
        }else{
        if($msg==null){
                msg('There is currently no message for this')
        }else{
                @newindex=array_push(@index,$msg)
                @newarray1=array_set(@array,index,@newindex)
                @newarray2=array_set(array_resize(@newarray1,array_size(@newarray1+1)),array_size(newarray1),$msg)
                store_value(buttons,announcements.broadcast,@newarray2)
        }
        }
<<<
